<h1 class="title is-1">Search Results</h1>

<div class="container">
<p> Your search for "<%= @searchterm %>" in <%= @choosencuisine %> found <%= @count %> results</p>
<div class="is-flex is-justify-content-flex-end">
    <%= paginate @restaurants %>
</div>
        <ul class="pl-3">
           <% @restaurants.each do |res|%>
                <li>
                <%= link_to res.RestaurantName, restaurant_path(res) %>
                </li>
           <% end %>
        </ul>
</div>